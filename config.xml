<?xml version='1.0' encoding='utf-8'?>
<widget id="librifox.mjsir911" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LibriFox</name>
    <description>
	Librivox audio book player
    </description>
    <author email="marco@sirabella.org" href="sirabella.org">
	Marco Sirabella
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <engine name="ubuntu" version="4.3.3" />
    <feature name="Media">
        <param name="id" value="cordova-plugin-media" />
        <param name="version" value="4.0.0" />
    </feature>
    <feature name="File Transfer">
        <param name="id" value="cordova-plugin-file-transfer" />
        <param name="version" value="1.7.1" />
    </feature>
    <feature name="File">
        <param name="id" value="cordova-plugin-file" />
        <param name="version" value="5.0.0" />
    </feature>
</widget>
